!
upgrade fpd auto
version 15.0
service timestamps debug datetime msec
service timestamps log datetime msec
no service password-encryption
!
hostname ABSR-1
!
boot-start-marker
boot-end-marker
!
!
no aaa new-model
!
!
!
ip source-route
no ip icmp rate-limit unreachable
ip cef
!
!
ip vrf UK
 rd 9000:10000
 route-target export 111.111.111.111:9000
 route-target import 9000:100
!
!
!
no ip domain lookup
ip multicast-routing 
no ipv6 cef
!
multilink bundle-name authenticated
!
!
!
!
!
!
!
!
!
redundancy
!
!
ip tcp synwait-time 5
! 
!
!
!
!
!
!
!
interface Loopback0
 description ROUTE-ID
 ip address 169.169.250.250 255.255.255.255
 ip pim sparse-mode
 ip ospf 10 area 0
 !
!
interface FastEthernet0/0
 mtu 1530
 ip address 169.169.4.1 255.255.255.252
 ip pim sparse-mode
 ip ospf mtu-ignore
 ip ospf prefix-suppression
 ip ospf 10 area 0
 duplex auto
 speed auto
 !
!
interface FastEthernet0/1
 mtu 1530
 ip address 169.169.5.2 255.255.255.252
 ip pim sparse-mode
 ip ospf mtu-ignore
 ip ospf prefix-suppression
 ip ospf 10 area 0
 duplex auto
 speed auto
 !
!
interface GigabitEthernet1/0
 mtu 4470
 no ip address
 negotiation auto
 !
!
interface GigabitEthernet1/0.1
 description OPTION_A_EBGP
 encapsulation dot1Q 1 native
 ip vrf forwarding UK
 ip address 91.91.91.1 255.255.255.252
 shutdown
!
interface GigabitEthernet1/0.2
 description OPTION_B_VPNV4_PEER
 encapsulation dot1Q 2
 ip address 91.91.91.5 255.255.255.252
 shutdown
 mpls bgp forwarding
!
interface GigabitEthernet1/0.3
 description OPTION_C_EBGP_MULTI_HOP
 encapsulation dot1Q 3
 ip address 91.91.91.9 255.255.255.252
 mpls bgp forwarding
!
interface GigabitEthernet2/0
 no ip address
 shutdown
 negotiation auto
 !
!
router ospf 10
 mpls ldp autoconfig area 0
 router-id 169.169.250.250
 log-adjacency-changes
 redistribute bgp 9000 subnets route-map OPTION_C_JAPAN_RR_LOOPBACK_192_168_253_253_32
!
router bgp 9000
 bgp router-id 169.169.250.250
 no bgp default ipv4-unicast
 no bgp default route-target filter
 bgp log-neighbor-changes
 neighbor 91.91.91.6 remote-as 1000
 neighbor 91.91.91.10 remote-as 1000
 neighbor 169.169.253.253 remote-as 9000
 neighbor 169.169.253.253 update-source Loopback0
 neighbor 169.169.254.254 remote-as 9000
 neighbor 169.169.254.254 update-source Loopback0
 !
 address-family ipv4
  no synchronization
  network 169.169.253.2 mask 255.255.255.255
  network 169.169.253.253 mask 255.255.255.255
  network 169.169.254.254 mask 255.255.255.255
  neighbor 91.91.91.10 activate
  neighbor 91.91.91.10 send-label
  no auto-summary
 exit-address-family
 !
 address-family vpnv4
  neighbor 91.91.91.6 activate
  neighbor 91.91.91.6 send-community extended
  neighbor 169.169.253.253 activate
  neighbor 169.169.253.253 send-community extended
  neighbor 169.169.253.253 next-hop-self
  neighbor 169.169.254.254 activate
  neighbor 169.169.254.254 send-community extended
  neighbor 169.169.254.254 next-hop-self
 exit-address-family
 !
 address-family ipv4 mdt
  neighbor 169.169.253.253 activate
  neighbor 169.169.253.253 send-community extended
  neighbor 169.169.254.254 activate
  neighbor 169.169.254.254 send-community extended
 exit-address-family
 !
 address-family ipv4 vrf UK
  no synchronization
  neighbor 91.91.91.2 remote-as 1000
  neighbor 91.91.91.2 activate
  neighbor 91.91.91.2 route-map TOKYO_111 in
 exit-address-family
!
ip forward-protocol nd
no ip http server
no ip http secure-server
!
!
ip pim ssm default
!
ip access-list standard TOKYO_111
 permit 111.111.111.111
!
!
ip prefix-list OPTION_C_JAPAN_RR_LOOPBACK_192_168_253_253_32 seq 5 permit 192.168.253.253/32
ip prefix-list OPTION_C_JAPAN_RR_LOOPBACK_192_168_253_253_32 seq 10 permit 192.168.251.251/32
!
!
!
!
route-map TOKYO_111 permit 10
 match ip address TOKYO_111
!
route-map OPTION_C_JAPAN_RR_LOOPBACK_192_168_253_253_32 permit 10
 match ip address prefix-list OPTION_C_JAPAN_RR_LOOPBACK_192_168_253_253_32
!
