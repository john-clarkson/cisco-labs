

feature telnet
feature nxapi
feature bash-shell
feature scp-server
cfs eth distribute
nv overlay evpn
feature ospf
feature bgp
feature pim
feature interface-vlan
feature vn-segment-vlan-based
feature vpc
feature lldp
feature nv overlay

fabric forwarding anycast-gateway-mac 1234.5678.9abc
ip pim rp-address 10.100.100.100 group-list 239.0.0.0/8
ip pim ssm range 232.0.0.0/8
vlan 1,100,200,1234
vlan 1234
  name L3VNI-Routing-TENANT1
  vn-segment 101234

vrf context L3-EXTERNAL-TENANT1
  vni 101234
  rd auto
  address-family ipv4 unicast
    route-target both auto
    route-target both auto evpn
  address-family ipv6 unicast
    route-target both auto
    route-target both auto evpn
vrf context management

interface Vlan1

interface Vlan100
  no shutdown
  vrf member L3-EXTERNAL-TENANT1
  ip address 10.1.1.1/24
  ipv6 address fc00:10:1:1::1/64

interface Vlan1234
  no shutdown
  vrf member L3-EXTERNAL-TENANT1
  ip forward
  ipv6 forward

interface nve1
  no shutdown
  host-reachability protocol bgp
  source-interface loopback0
  member vni 101234 associate-vrf

interface Ethernet1/3
  switchport mode trunk
  switchport trunk allowed vlan 100

line vty
boot nxos bootflash:/nxos.7.0.3.I7.2.bin 

router bgp 65511
  router-id 10.0.0.102
  address-family ipv4 unicast
  address-family ipv6 unicast
  address-family l2vpn evpn
  neighbor 10.0.0.1
    remote-as 65511
    update-source loopback0
    address-family ipv4 unicast
    address-family l2vpn evpn
      send-community extended
  vrf L3-EXTERNAL-TENANT1
    address-family ipv4 unicast
      advertise l2vpn evpn
    address-family ipv6 unicast
      advertise l2vpn evpn
    neighbor fc00:10:1:1::254
      remote-as 65599
      update-source Vlan100
      address-family ipv6 unicast
    neighbor 10.1.1.254
      remote-as 65599
      update-source Vlan100
      address-family ipv4 unicast

copy run s
